<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    tools:context=".LoyaltyBankActivity">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="360dp"
        android:layout_height="96dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025">

        <Button
            android:id="@+id/button"
            android:layout_width="93dp"
            android:layout_height="59dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="100dp"
            android:background="#00000000"
            android:backgroundTint="#F1F1F1"
            android:text="Button"
            app:icon="@drawable/ic_plus"
            app:iconSize="50dp"
            app:iconTint="#4D4D4D" />

        <Button
            android:id="@+id/button2"
            android:layout_width="126dp"
            android:layout_height="53dp"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="-60dp"
            android:background="#00000000"
            android:backgroundTint="#00000000"
            android:text="Button"
            app:icon="@drawable/ic_eye"
            app:iconPadding="50dp"
            app:iconSize="60dp"
            app:iconTint="#4D4D4D" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="100dp"
            android:fontFamily="@font/montserrat"
            android:text="Add route/tour "
            android:textColor="#4D4D4D"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="163dp"
            android:layout_height="44dp"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="-40dp"
            android:fontFamily="@font/montserrat"
            android:text="View suggested tours"
            android:textColor="#4D4D4D"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="192dp"
        android:layout_height="183dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.341">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="187dp"
            android:layout_height="122dp"
            android:layout_marginLeft="15dp"
            app:srcCompat="@drawable/ic_loogo" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:fontFamily="@font/montserrat_bold"
            android:text="Tourybank"
            android:textColor="#388084"
            android:textSize="24sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="325dp"
        android:layout_height="80dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.677">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/montserrat"
            android:text="Your balance: 3400"
            android:textColor="#4D4D4D"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="92dp"
            android:layout_height="65dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="-58dp"
            android:backgroundTint="#388084"
            app:srcCompat="@drawable/ic_piggy"
            app:tint="#4D4D4D" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="358dp"
        android:layout_height="115dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.886"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.988">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="220dp"
            android:fontFamily="@font/montserrat_light"
            android:text="tourycoins"
            android:textColor="#388084"
            android:textSize="18sp" />

        <androidx.cardview.widget.CardView
            android:layout_width="173dp"
            android:layout_height="62dp"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="10dp"
            app:cardBackgroundColor="#F1F1F1"
            app:cardCornerRadius="25dp"
            android:clickable="true"
            android:focusable="true"
            android:onClick="underDevAlertOnClickMethod">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="EXCHANGE"
                android:textColor="#4D4D4D" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>