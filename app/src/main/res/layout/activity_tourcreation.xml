<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="52dp"
        android:layout_marginTop="25dp"
        android:text=" Add your  route option"
        android:textColor="#388084"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="44dp" />

    <LinearLayout
        android:layout_width="285dp"
        android:layout_height="55dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/rounded_background1"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="84dp" >

        <EditText
            android:id="@+id/editTextText2"
            android:layout_width="297dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="text"
            android:text="Name" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="enter the name of the tour/route/event"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="131dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="3dp"/>

    <LinearLayout
        android:layout_marginStart="2dp"
        android:layout_marginTop="20dp"
        android:layout_width="350dp"
        android:layout_height="53dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="110dp"
            android:layout_height="44dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="2dp"
            android:background="@drawable/rounded_background1"

            android:orientation="vertical"
            tools:layout_editor_absoluteX="52dp"
            tools:layout_editor_absoluteY="195dp">


            <EditText
                android:id="@+id/editTextText3"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:ems="10"
                android:inputType="text"
                android:text="time" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="44dp"
            android:layout_marginStart="100dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/rounded_background1"

            android:orientation="vertical"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="195dp">

            <EditText
                android:id="@+id/editTextText4"
                android:layout_width="103dp"
                android:layout_height="55dp"
                android:ems="10"
                android:inputType="text"
                android:text="price" />
        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:orientation="horizontal">

        <TextView

            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="enter the time"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="247dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="3dp"/>

        <TextView
            android:layout_marginStart="120dp"
            android:layout_marginTop="3dp"
            android:id="@+id/textView6"
            android:layout_width="93dp"
            android:layout_height="19dp"
            android:text="enter the price"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="247dp" />
    </LinearLayout>


    <LinearLayout
        android:layout_marginStart="52dp"
        android:layout_marginTop="10dp"
        android:layout_width="240dp"
        android:layout_height="100dp"
        android:background="@drawable/rounded_background1"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.591"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533">


        <EditText

            android:id="@+id/editTextText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:ems="10"
            android:inputType="text"
            android:text="Description" />
    </LinearLayout>

    <TextView
        android:layout_marginStart="35dp"
        android:layout_marginTop="1dp"
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="enter the dedcription of the tour/route/event"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="400dp" />

    <LinearLayout
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:layout_width="123dp"
        android:layout_height="128dp"
        android:background="@drawable/rounded_background1"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="444dp">


        <com.google.android.material.imageview.ShapeableImageView
            android:layout_marginStart="28dp"
            android:layout_marginTop="30dp"
            android:id="@+id/picture"
            android:layout_width="68dp"
            android:layout_height="65dp"

            android:background="#B2B1B2"
            android:scaleType="centerCrop"
            android:src="@drawable/picture1"
            app:tint="#4D4D4D"
            tools:layout_editor_absoluteX="76dp"
            tools:layout_editor_absoluteY="476dp" />
    </LinearLayout>

    <TextView
        android:layout_marginStart="285dp"
        android:layout_marginTop="-40dp"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:text="DONE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.859" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/done"
        android:layout_width="38dp"
        android:layout_height="37dp"
        android:layout_marginStart="285dp"
        android:layout_marginTop="-4dp"

        android:scaleType="centerCrop"
        android:src="@drawable/done1"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="462dp" />

    <TextView
        android:layout_marginStart="52dp"
        android:layout_marginTop="-10dp"
        android:id="@+id/textView9"
        android:layout_width="97dp"
        android:layout_height="17dp"

        android:layout_marginBottom="48dp"
        android:text="add photos"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="81dp" />


</LinearLayout>
